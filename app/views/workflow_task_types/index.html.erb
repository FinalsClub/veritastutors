<h1>Listing workflow_task_types</h1>

<table>
  <tr>
    <th>Owner role</th>
    <th>Partial</th>
  </tr>

<% for workflow_task_type in @workflow_task_types %>
  <tr>
    <td><%=h workflow_task_type.owner_role_id %></td>
    <td><%=h workflow_task_type.partial %></td>
    <td><%= link_to 'Show', workflow_task_type %></td>
    <td><%= link_to 'Edit', edit_workflow_task_type_path(workflow_task_type) %></td>
    <td><%= link_to 'Destroy', workflow_task_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New workflow_task_type', new_workflow_task_type_path %>
